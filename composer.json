{
  "name": "revolution/laravel-amazon-bedrock",
  "description": "Tiny Amazon Bedrock wrapper for Laravel",
  "keywords": [
    "laravel",
    "amazon-bedrock"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "illuminate/support": "^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^10.0",
    "laravel/pint": "^1.22",
    "pestphp/pest": "^4.1",
    "pestphp/pest-plugin-laravel": "^4.0",
    "revolution/laravel-boost-copilot-cli": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Amazon\\Bedrock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Workbench\\App\\": "workbench/app/",
      "Workbench\\Database\\Factories\\": "workbench/database/factories/",
      "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    },
    {
      "name": "puklipo"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Amazon\\Bedrock\\BedrockServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "vendor/bin/pest",
    "test:coverage": "pest --coverage --coverage-clover build/logs/clover.xml",
    "lint": "pint",
    "test:lint": "pint --test",
    "post-autoload-dump": [
      "@clear",
      "@prepare"
    ],
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": "@php vendor/bin/testbench workbench:build --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve --ansi"
    ]
  }
}
